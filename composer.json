{
    "name": "roussks/financial-year-carbon",
    "description": "This is the Carbon Adapter implementation of roussks/financial-year library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RoussKS",
            "homepage": "https://www.roussks.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RoussKS\\FinancialYear\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoussKS\\FinancialYear\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "roussks/financial-year": "1.1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.8",
        "phpunit/phpunit": "^7 || ^8"
    }
}
